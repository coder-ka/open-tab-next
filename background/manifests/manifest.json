{
  "name": "Open a new tab next to the current tab",
  "description": "Open a new tab next to the current tab.",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "background.umd.js"
  },
  "permissions": ["tabs"],
  "commands": {
    "open-new-tab-next": {
      "suggested_key": {
        "default": "Alt+T"
      },
      "description": "Open a new tab next to the current tab."
    }
  },
  "icons": {
    "16": "/images/icon_16.png",
    "32": "/images/icon_32.png",
    "48": "/images/icon_48.png",
    "128": "/images/icon_128.png"
  }
}
